    <string name="title_activity_settings">Settings</string>


    <string name="today">Today</string>
    <string name="tomorrow">Tomorrow</string>
    <string name="format_full_friendly_date">
        <xliff:g id="day">%1$s</xliff:g>, <xliff:g id="date">%2$s</xliff:g>
    </string>

    <string name="sync_account_type">haris.app.myschedule</string>
    <string name="content_authority">haris.app.myschedule.sync.MyScheduleSyncAdapter</string>

